    "url": "/project/create/",
    "title": "Create a new project",
    "description": "<p>Creates a new titra project based on the parameters provided</p>",
    "name": "CreateProject",
    "group": "Project",
    "header": {
      "fields": {
        "Header": [
          {
            "group": "Header",
            "type": "String",
            "optional": false,
            "field": "Token",
            "description": "<p>The authorization header Bearer API token.</p>"
          }
        ]
      }
    },
